mixin like(obj)
  - let baseClass = 'like';
  label(class=`${baseClass}`)
    input(class=`${baseClass}__input` type='checkbox')&attributes(attributes)
    span(class=`${baseClass}__mark`)
      div(class=`${baseClass}__icon`)
        include ./like.svg
      span(class=`${baseClass}__label`)=obj.num
      